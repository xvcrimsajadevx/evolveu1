<script type="text/javascript">

function assertEquals(a,b) {
    //console.log(a, b);
    
    if (a === b) {
        console.log("Okay =>", a);
        return true;
    } else {
        console.log("the two values are not the same:")
        console.log("p1->", a);
        console.log("p2->", b);
        
        return false;
    }
}

</script>